<%- include("partials/header") -%>

<h1>Home</h1>
<p>
    <%= homeStartContent %>
    <% posts.forEach(function(post){ %>
        <h1><%= post.name %></h1>
        <p><%= post.content.substring(0,60)+"... " %><a href="/posts/<%= post._id %>">Read More</a></p>
    <% }) %>
</p>

